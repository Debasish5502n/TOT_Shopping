<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:layout_marginStart="8dp"
        android:orientation="vertical"
        android:elevation="3dp"
        android:background="#EDEDED"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:background="#EDEDED"
        android:elevation="3dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:layout_marginTop="16dp"
        android:background="#EDEDED"
        android:orientation="vertical"
        android:elevation="3dp"
        app:layout_constraintStart_toEndOf="@+id/linearLayout11"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:layout_marginTop="16dp"
        android:background="#EDEDED"
        android:orientation="vertical"
        android:elevation="3dp"
        app:layout_constraintStart_toEndOf="@+id/linearLayout12"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:layout_marginTop="16dp"
        android:background="#EDEDED"
        android:orientation="vertical"
        android:elevation="3dp"
        app:layout_constraintStart_toEndOf="@+id/linearLayout13"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout112"
        android:layout_width="60dp"
        android:layout_height="75dp"
        android:layout_margin="16dp"
        android:layout_marginTop="16dp"
        android:background="#EDEDED"
        android:orientation="vertical"
        android:elevation="3dp"
        app:layout_constraintStart_toEndOf="@+id/linearLayout14"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="8dp"
            android:src="@drawable/ny_home"
            app:tint="#CA5454" />
        <View
            android:layout_width="50dp"
            android:layout_height="10sp"
            android:layout_gravity="center"
            android:background="#F6F6F6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout15"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/border_button_gray"
        android:elevation="3dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout16"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:background="#EDEDED"
        android:elevation="3dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout15">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_margin="16dp"
        android:background="@drawable/border_button_gray"
        android:elevation="3dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

        <Button
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:layout_gravity="end"
            android:layout_margin="8dp"
            android:backgroundTint="#F69292"
            android:text="view all"
            android:textColor="#EDEDED" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_margin="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/border_button_gray"
        android:elevation="3dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout17">

        <Button
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:layout_gravity="end"
            android:layout_margin="8dp"
            android:backgroundTint="#F69292"
            android:text="view all"
            android:textColor="#EDEDED" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>